name: Hello world job
 
on: 
  push:
    branches:
      - main
jobs:
  hello-world:
    runs-on: ubuntu-latest
    steps:
       - name: Echo Hello
         run: echo "hello world"
 
 
  how-are-you:
     runs-on: ubuntu-latest
     steps:
       - name: ask how the person is
         run: echo "how are you?"    
